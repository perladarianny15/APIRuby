<!DOCTYPE html>
<html>
  <head>
    <title>TravelApi</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Travelwise</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <meta name="author" content="" />

  </head>

  <body>
      <div class="container">	
        <%= render 'layouts/messages' %>		     
      </div>
      <%= yield %>
      <!-- jQuery -->
      <%= javascript_include_tag "jquery.min" %>
      <!-- jQuery Easing -->
      <%= javascript_include_tag "jquery.easing.1.3" %>
      <!-- Bootstrap -->
        <%= javascript_include_tag "bootstrap.min" %>
      <!-- Waypoints -->
      <%= javascript_include_tag "jquery.waypoints.min" %>
      <!-- Flexslider -->
      <%= javascript_include_tag "jquery.flexslider-min" %>
      <!-- Owl carousel -->
      <%= javascript_include_tag "owl.carousel.min" %>
      <!-- Magnific Popup -->
      <%= javascript_include_tag "jquery.magnific-popup.min" %>
      <%= javascript_include_tag "magnific-popup-options" %>
      <!-- Date Picker -->
      <%= javascript_include_tag "bootstrap-datepicker" %>
      <!-- Stellar Parallax -->
      <%= javascript_include_tag "jquery.stellar.min" %>
      <!-- Main -->
      <%= javascript_include_tag "main" %>
      <%= javascript_include_tag "respond.min" %>

  </body>
</html>
