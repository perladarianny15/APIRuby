<div class="container">
    <div class="row">
        <div class="col-lg-12 mt-5">
            <div class="mx-auto mt5" style="width: 900px">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                <tr>
                    <th>Type </th>
                    <th>Origin</th>
                    <th>Destination</th>
                    <th>Departure Date</th>
                    <th>Return Date</th>
                    <th>Price</th>
                </tr>
                </thead>
                <tfoot>
                <tr>
                    <th>Type </th>
                    <th>Origin</th>
                    <th>Destination</th>
                    <th>Departure Date</th>
                    <th>Return Date</th>
                    <th>Price</th>
                </tr>
                </tfoot>
                <tbody>
                <% @FlightInsp.each do |flight| %>
                <tr>
                    <td><%= flight['type']%></td>
                    <td><%= flight['origin']%></td>
                    <td><%= flight['destination']%></td>
                    <td><%= flight['departureDate']%></td>
                    <td><%= flight['returnDate']%></td>
                    <td><%= flight['price']['total']%></td>
                </tr>
                <% end %>
            </tbody>
            </table>
            </div>
        </div>
    </div>
</div>